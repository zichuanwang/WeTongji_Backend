<h1>Listing school_news</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Source</th>
    <th>Summary</th>
    <th>Context</th>
    <th>Read</th>
    <th>Like</th>
    <th>Favorite</th>
    <th>Admin</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @school_news.each do |school_news| %>
  <tr>
    <td><%= school_news.title %></td>
    <td><%= school_news.source %></td>
    <td><%= school_news.summary %></td>
    <td><%= school_news.context %></td>
    <td><%= school_news.read %></td>
    <td><%= school_news.like %></td>
    <td><%= school_news.favorite %></td>
    <td><%= school_news.admin_id %></td>
    <td><%= link_to 'Show', school_news %></td>
    <td><%= link_to 'Edit', edit_school_news_path(school_news) %></td>
    <td><%= link_to 'Destroy', school_news, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New School news', new_school_news_path %>
