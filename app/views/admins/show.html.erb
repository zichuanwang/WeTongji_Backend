<div class="box">
    <div class="title">
        <h5>管理员详情</h5>
    </div>
    <div class="table">
        <table>
          <tr>
            <td>邮箱</td>
            <td class="last"><%= @admin.email %></td>
          </tr>
          <tr>
            <td>角色</td>
            <td class="last"><%= @admin.role %></td>
          </tr>
          <tr>
            <td>创建时间</td>
            <td class="last"><%= @admin.created_at.strftime("%Y/%m/%d %H:%M:%S") %></td>
          </tr>
          <tr>
            <td>登陆次数</td>
            <td class="last"><%= @admin.sign_in_count %></td>
          </tr>
        </table>
    </div>
</div>